#!groovy

pipeline 
{
    agent any
    
    options {
          timestamps()
    }

    stages {
        
        stage("Build")
        {
            steps{
                
                     echo "Sunt aici etapa Build"
            }
        }
        
        stage("Testing")
        {
            steps{
                    echo "Sunt aici etapa Test"
                    bat './mvnw test'
            }
        }
        
        
    }

}
